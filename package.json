    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt build && nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "e2e:run": "playwright test",
    "e2e:run:watch": "playwright show-report",
    "e2e:run:ui": "playwright test --ui",
    "type:check": "nuxi typecheck",
