    "@nuxt/eslint": "0.7.6",
    "@nuxt/eslint-config": "^0.7.4",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.1",
    "@playwright/test": "^1.49.1",
